page{
  background-color: #f4f4f4;
}
.addContainer{
  width: 100%;
  height: 1111rpx;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
}
.address{
  width: 710rpx;
  height: 175rpx; 
  padding: 20rpx 20rpx 20rpx 20rpx;
  margin-top: 20rpx;
  background-color: #fff;
}
.location{
  width: 30rpx;
  height: 30rpx;
  margin: 45rpx 10rpx 30rpx 0;
  float: left;
}
.consignee{
  font-size: 30rpx;
  width: 335rpx;
  height: 35rpx;
  line-height: 35rpx;
  float: left;
}
.phoneNumber{
  font-size: 30rpx;
  width: 335rpx;
  height: 35rpx;
  line-height: 35rpx;
  float: left;
  text-align: right;
}
.detailedAddress{
  font-size: 24rpx;
  width: 670rpx;
  height: 60rpx;
  line-height: 30rpx;
  margin: 10rpx 0 0 0;
  float: left;
}
.editColumn{
  height: 60rpx;
  width: 100%;
  line-height: 60rpx; 
  font-size: 24rpx;
  float: left;
  margin-top: 10rpx;
}
.editColumn image{
  width: 25rpx;
  height: 25rpx;
  margin-right: 16rpx;
}
.setDefault{
  float: left;
  color: #a9a9a9;
}
.delete{
  float: right;
  margin-left: 60rpx;
}
.edit{
  float: right;
}

.newAddress{
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  position: fixed;
  bottom: 0;
  color: #fff;
  background-color: #ff0033;
}