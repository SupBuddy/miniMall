page {  
  background-color:#f4f4f4;  
  overflow: scroll;
} 
.header{
  width: 100%;
  height: 210rpx;
}
.qrCode{
  width: 40rpx;
  height: 40rpx;
  margin: 85rpx 40rpx 85rpx 50rpx;
  float: left;
}
.userInfo{
  width: 400rpx;
  height: 210rpx;
  float: left;
  font-size: 36rpx;
}
.userInfo text{
  height: 60rpx;
  width: 400rpx;
  float: left;
  line-height: 60rpx;
}
.userName{
  margin-top: 45rpx;
}
.phoneNum{
  margin-bottom: 45rpx;
}
.userAvatar{
  width: 140rpx;
  height: 140rpx;
  float: right;
  margin: 30rpx;
  border: solid 2px #ff0033;
  border-radius: 100px;
}

.section{
  width: 100%;
  margin-bottom: 40rpx;
}
.item{
  width: 100%;
  height: 150rpx;
  line-height: 150rpx;
  margin-bottom: 1px;
  font-size: 36rpx;
  background-color: #ffffff;
}
.itemIcon{
  width: 80rpx;
  height: 80rpx;
  float: left;
  margin: 35rpx 50rpx;
}
.item label{
  float: left;
}
.enterIcon{
  width: 60rpx;
  height: 60rpx;
  float: right;
  margin: 45rpx 50rpx;
}


