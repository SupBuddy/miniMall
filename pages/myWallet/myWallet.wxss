page{
  background-color: #f4f4f4;
}
.walletContainer{
  width: 100%;
  margin-top: 30rpx;
}
.item{
  width: 700rpx;
  padding: 0 25rpx;
  margin-bottom: 20rpx;
  font-size: 30rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #fff;
}
.item image{
  float: left;
  height: 50rpx;
  width: 50rpx;
  margin: 20rpx 0;
}
.item label{
  float: left;
  width: 200rpx;
  margin-left: 25rpx;
}
.item text{
  float: left;
  width: 425rpx;
  text-align: right;
  color: #ff0090;
}